<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TredgateMauiTestingApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TredgateMauiTestingApp"
    Shell.FlyoutBehavior="Disabled"
    Title="TredgateMauiTestingApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent Route="EditTaskPage"
                  ContentTemplate="{DataTemplate local:EditTaskPage}" />
    <ShellContent Route="WebViewPage"
                  ContentTemplate="{DataTemplate local:WebViewPage}" />


</Shell>
